								#ifdef WRITE_MSAA_DEPTH
							, out float1 depthColor : SV_Target1
								#endif
							#else
							, out float4 outColor : SV_Target0
							#endif

							#ifdef _DEPTHOFFSET_ON
							, out float outputDepth : SV_Depth
							#endif
							
						)
				{
					FragInputs input;
					ZERO_INITIALIZE(FragInputs, input);
					input.worldToTangent = k_identity3x3;
					input.positionSS = packedInput.positionCS;

					PositionInputs posInput = GetPositionInput(input.positionSS.xy, _ScreenSize.zw, input.positionSS.z, input.positionSS.w, input.positionRWS);

					float3 V = float3(1.0, 1.0, 1.0); // Avoid the division by 0

					SurfaceData surfaceData;
					BuiltinData builtinData;
					SurfaceDescription surfaceDescription = (SurfaceDescription)0;
					float2 uv08 = packedInput.ase_texcoord.xy * _Tiling + ( _Time.y * _Offset );
					float2 uv_TextureSample2 = packedInput.ase_texcoord.xy * _TextureSample2_ST.xy + _TextureSample2_ST.zw;
					float4 tex2DNode26 = tex2D( _TextureSample2, uv_TextureSample2 );
					float2 appendResult30 = (float2(tex2DNode26.r , tex2DNode26.g));
					float2 uv021 = packedInput.ase_texcoord.xy * _Vector1 + ( _Time.y * _Vector0 );
					float4 temp_output_14_0 = ( ( ( tex2D( _Colour, ( uv08 + ( 0.025 * appendResult30 ) ) ).r * 0.5 ) + tex2D( _Colour, ( ( appendResult30 * 0.05 ) + uv021 ) ) ) + _Alpha );
					float4 clampResult16 = clamp( temp_output_14_0 , float4( 0,0,0,0 ) , float4( 1,0,0,0 ) );
					
